<?xml version='1.0' encoding='UTF-8'?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:vdldoc="http://vdldoc.org/vdldoc"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd http://vdldoc.org/vdldoc https://raw.githubusercontent.com/omnifaces/vdldoc/master/src/main/resources/org/omnifaces/vdldoc/resources/vdldoc.taglib.xml.xsd"
	version="2.0">
	<description><![CDATA[Proof of Concept: Soy Templates in JSF Renderers.]]></description>
	<namespace>http://liferay.com/faces/poc</namespace>
	<tag>
		<description><![CDATA[Says hello to the name supplied by the value attribute.]]></description>
		<tag-name>outputHello</tag-name>
		<component>
			<component-type>com.liferay.faces.soy.poc.OutputHello</component-type>
			<renderer-type>com.liferay.faces.soy.poc.SoyTemplateRenderer</renderer-type>
		</component>
		<attribute>
			<description><![CDATA[The name of the person who we are saying hello to.]]></description>
			<name>value</name>
			<required>true</required>
			<type>java.lang.Object</type>
		</attribute>
	</tag>
</facelet-taglib>
